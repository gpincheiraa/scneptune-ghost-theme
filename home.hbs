            </svg>
          </div>
          <div class="flex-grow-1 v-mid pl2">
            <p class="f5 f7-m fw2 lh-title mv0">
            </p>
            <p class="f6 lh-copy fw2">
              With over seven years of web application development,
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="landing_timeline brand-secondary-hightlight dn db-ns cover bg-left bg-center-l" style="background-image: url('{{asset "img/code-bg.png"}}')">
    <div class="center mw8 pv4 ph4-ns ph3 cf">
      {{> timeline}}
    </div>
  </section>
  <!-- Social Feed -->
  <section class="landing_socialfeed bg-color">
    <div class="center mw8 pv4 ph4-ns ph3 cf">
      <div class="overflow-hidden">
        <div class="" style="width: 100vw;" >
          {{> social-post}}
        </div>
      </div>
    </div>
  </section>

  <section class="landing_work brand-primary">
    <div class="center mw8 pv4 ph4-ns ph3 cf">
      <div class="cf w-100 tl">
        <h4 class="f4 fw6 lh-title">
          Work
        </h4>
      </div>
      {{> work-loop}}
    </div>
  </section>


  <!-- Recent Loop -->
  <section class="landing_recentposts bg-color">
    <div class="center mw8 pv4 ph4-ns ph3 cf">
      <div class="cf w-100">
        <h4 class="f4 fw6 lh-title mb2 center tl">
          Writing
        </h4>
      </div>
      {{> recent-loop}}
    </div>
  </section>

  <section class="landing_stats brand-primary">
    <div class="center mw8 pv4 ph4-ns ph3 cf">
    </div>
  </section>
</div>
